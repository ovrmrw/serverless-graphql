  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "sourceMap": false,
    "strictNullChecks": true,
    "skipLibCheck": true,
    "lib": [
      "es2017",
      "dom"
    ]
  },
  "exclude": [
    "dest",
    "node_modules"
  ]
}